#include <iostream>
using namespace std;
class node{
	int data;
	node* next;
};
class queue{
	node* head;
   public:
    queue(int a){
    	head = new node;
    	head -> data = a;
    	head -> next = NULL;
    }
    void push(int a){
    	node* temp = new node;
    	temp -> next = head;
    	temp -> data = a;
    	head = temp;
    }
    int pop(){
    	node* temp = head;
    	int a;
    	while(temp -> next -> next != NULL){
    	    temp = temp -> next;
    	}
    	a = temp -> next -> data;
    	delete temp -> next;
    	temp -> next = NULL;
    	return a;
    }
    ~queue(){
    	while(head != NULL){
    		node* temp = head;
    		head = head -> next;
    		delete temp;
    	}
    }
}
