#include <iostream>
using namespace std;
int main(){
  int n, t = 1;
  cin >> n;
  int* A = new int[n];
  int* B = new int[n];
  int* C = new int[n];
  for(int i = 0; i < n; i++)
    cin >> A[i] >> B[i];
  int x, y;
  double k = (B[0]-B[n-1])/(A[0]-A[n-1]);
  if(A[n-1]*k > B[n-1])
    C[n-1] = 1;
  else C[n-1] = -1;
  cout << k << " ";
  int s, s1;
  for(int i = 0; i < n-1; i++){
    x = A[i+1] - A[i];
    y = B[i+1] - B[i];
    k = y/x;
    cout << k << " ";
    for(int j = 0; j < n-1; j++)
      if(A[j]*k > B[j])
        C[j] = 1;
      else C[j] = -1;
    for(int j = 0; j < n; j++){
      s += abs(C[j]);
      s1 += C[j];
    }
    for(int j = 0; j < n; j++)
      cout << C[j] << " ";
    if(s != abs(s1))
      t = 0;
    cout << endl;
  }
  //cout << t;
  return 0;
}
