#include <iostream>
using namespace std;
void spisok_smew(int* A, int* B, int n, int m){
	for(int t = 1; t <= m; t++){
		cout << t << ":" << " ";
	  for(int i = 0; i < n; i++){
	    if(A[i] == t)
	      cout << B[i] << " ";
	    if(B[i] == t)
	      cout << A[i] << " ";
	  }
	  cout << endl;
	}
}
int main(){ 
	int n, max = 0;
	cin >> n;
	int* A = new int(n);
	int* B = new int(n);
  for(int i = 0; i < n; i++)
    cin >> A[i] >> B[i];
  for(int i = 0; i < n; i++){
    if(max < A[i])
      max = A[i];
    if(max < B[i])
      max = B[i];
  }
  spisok_smew(A, B, n, max);
  cout << endl;
  return 0;
}
