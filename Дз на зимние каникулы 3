#include <iostream>
#include <vector>
using namespace std;
void SimpleCountingSort(vector <int> &A, int k, int n){
	vector <int> cnt(k);
 for (int i = 0; i < n; i++) cnt[A[i]]++;
 for (int j = k - 1; j >= 0; j--) {
  for (int i = 0; i < cnt[j]; i++){
   cout << j << ' ';
  }
 }
}
int main(){
	int n, k;
	cin >> n >> k;
	vector <int> A(n);
	for (int i = 0; i < n; i++)
	  cin >> A[i];
	SimpleCountingSort(A, k, n);
	return 0;
}
