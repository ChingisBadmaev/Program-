#include <iostream> 
using namespace std; 

void print(int **V){
	for(int i = 0; i < 8; i++){
	  for(int l = 0; l < 4; l++)
	    cout << V[i][l] << " ";
	  cout << endl;
	}
}

int prov(int **V, int i, int l){
	if(i == 7)
	  return -1;
	if(i % 2 == 0)
		if((V[i+1][l] == 1 || i > 6) && (V[i+1][l+1] == 1 || l > 2)) 
		    if((V[i-1][l] == 1 || i < 1) && (V[i-1][l+1] == 1 || i < 1))
		        return 1;
	else
		if((V[i+1][l] == 1 || i > 6) && (V[i+1][l-1] == 1 || l < 1))
		    if((V[i-1][l] == 1 || i < 1) && (V[i-1][l-1] == 1 || i < 1))
	            return 1;
	return 0;
}

int ocen(int **V){
    for(int i = 0; i < 8; i++)
	  for(int l = 0; l < 4; l++)
	    if(V[i][l] == -4)
	      return prov(V, i, l);
}

