int main() {
  int **Table = new int *[8];
  for(int i = 0; i < 8; i++)
    Table[i] = new int[4];
  for(int i = 0; i < 8; i++)
    for(int l = 0; l < 4; l++)
      Table[i][l] = 0;
  Table[0][2] = -4;
  //for(int i = 0; i < 4; i++)
    //Table[7][i] = 1;
  Table[1][3] = 1; Table[1][2] = 1;
  print(Table);
  cout << ocen(Table) << endl;
  //system("pause");
  return 0;
}
