#include <iostream>
using namespace std;
int main(){
  string A = "tikiiiiijiyieikieiyiekiyieikijikiyieijitikieiyijjektkiekiheieekyeehikikut";
  int n = 0, k = 0, j = 0, j1 = 0, j2 = 0, g, h;
  while(A[n] != '\0')
    n++;
  char** B = new char*[n];
  for(int i = 0; i < n; i++)
    B[i] = new char[n];
  int* C = new int[n];
  for(int i = 0; i < n; i++){
    for(int l = 0; l < i; l++)
      if(A[i] == B[l][0]){
        k = 1;
        g = l;
      }
    if(k == 0){
      B[j][0] = A[i];
      j++;
    }
    else C[g]++;
    k = 0;
  }
  for(int i = 0; i < j; i++)
    C[i]++;
    
  int** D = new int*[j];
  for(int i = 0; i < j; i++)
    D[i] = new int[j];
  for(int i = 0; i < j; i++)
    for(int l = 0; l < j; l++)
      D[i][l] = -1;
  h = j;
  for(int i = 0; i < j-1; i++){
    for(int f = 0; f < h; f++)
      for(int l = f; l < h; l++)
        if(C[f] < C[l]){
          swap(C[f], C[l]);
          swap(B[f], B[l]);
          swap(D[f], D[l]);
        }
    D[h-1][i] = 0;
    D[h-2][i] = 1;
    C[h-2] += C[h-1];
    while(B[h-2][j1] != '\0'){
      j1++;
    }
    while(B[h-1][j2] != '\0'){
      B[h-2][j1] = B[h-1][j2];
      j1++;
      j2++;
    }
    
    j1 = 0;
    j2 = 0;
    h--;
  }
  for(int i = 0; i < j; i++){
    cout << B[i][0] << " ";
    for(int l = j-1; l >= 0; l--)
      if(D[i][l] != -1)
        cout << D[i][l] << " ";
    cout << endl;
  }
  return 0;
}
