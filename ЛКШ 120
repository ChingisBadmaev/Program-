#include <stdio.h>
using namespace std;
int main(){
  int n, g, h, l, k = 0, j1 = 1, t = 0;
  scanf("%d", &n);
  int* A = new int[n+2];
  int* T = new int[n+2];
  int* S = new int[n+2];
  A[0] = -1;
  S[0] = 0;
  T[0] = 0;
  for(int i = 1; i <= n; i++)
  	scanf("%d%d%d", &A[i], &S[i], &T[i]);
  for(int i = 1; i <= n; i++){
    while(S[i] > t){
      while(A[j1] == 0)
        j1++;
      A[j1] = -1;
      if(S[j1] > t)
       t = S[j1] + T[j1];
      else t += T[j1];
      j1++;
    }
    if(A[i-1] != -1)
      if(A[i-1] == A[i])
        k = 1;
      else 
        if(k == 1){
          g = i - 2;
          while(A[g] == A[i-1]){
            A[g] = 0;
            g--;
          }
          A[i-1] = 0;
          if(A[g] != A[i])
            k = 0;
        }
  }
  for(int i = 1; i <= n; i++)
    if(A[i] != 0)
      printf("% d", i);
  return 0;
}
