#include <iostream>
using namespace std;
int* used;
int main(){
	used = new int[m];
	for(int i = 0; i < m; i++)
	  if(used[j] == 0)
	    Q.pop(j);
	while(Q.empty){
	  int t = Q.pop();
	  for(int i = 0; i < m; i++)
	    if(used[i] == 0){
	      used[i] = 3 - used[t];
	      Q.push(i);
	    }
	    else if(used[i] == used[t])
	      return;
	}
	return 0;
}
