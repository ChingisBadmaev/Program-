#include <iostream>
using namespace std;
int resh(int n){
  int s = 0;
  int* prime = new int[n+1];
  prime[0] = 1;
  prime[1] = 1;
  for(int i = 2; i <= n/2+1; i++)
    for(int j = i*i; j <= n; j += i)
      prime[j] = 1;
  for(int i = 2; i <= n; i++)
    if(prime[i] == 0)
      s++;
  return s;
}
int main(){
  int n;
  cin >> n;
  int s = n*(n+1)/2;
  cout << resh(n) << " " << s;
  return 0;
}
