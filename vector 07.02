#include <iostream>
using namespace std;
class vector{
 int* Array;
 int size;
 int cap;
  public:
   vector(int n){
     Array = new int[n];
     for (int i = 0; i < n; i++)
      Array[i] = '\0';
     size = n;
     cap = 0;
   }
   void push(int a){
     if (size == cap){
      int* Array1 = new int[size*2];
      for (int i = 0; i < size; i++)
       Array1[i] = Array[i];
      Array1[cap+1] = a;
      cap++;
      size *= 2;
      return;
     }
     Array[cap+1] = a;
     cap++;
   }
   int size_now(){
     return cap;
   }
   int find(int a){
     int i = 0;
     while(a!=Array[i]&&Array[i+1]!='\0')
      i++;
     if(a == Array[i])
      return i;
     return -1;
   }
   void append(int new_size){}
   int operator[](int i){}
   const int& operator[](int i) const{}
   ~vector(){
     delete Array;
   }
};
int main(){
 int n;
 cin >> n;
 vector obj1(n);
 obj1.push(n);
 obj1.push(n+1);
 obj1.push(n+2);
 cout << obj1.size_now() << endl;
 cout << obj1.find(n+1) << endl;
 return 0;
}
