#include <iostream>
using namespace std;
int** C;
int* A;
int* B;
int* used;
int* D;
int n, m;
void matrix_smew(){
  C = new int*[m];	
  for(int i = 0; i < m; i++)
    C[i] = new int[m];
  for(int l = 0; l < m; l++)
    for(int i = 0; i < n; i++)
      C[l][i] = -1;
  for(int i = 0; i < n; i++)
    C[A[i]][B[i]] = D[i];
}
int main(){
  cin >> n >> m;//n-кол-во ребер, m-вершин
  A = new int[n];
  B = new int[n];
  D = new int[n];
  for(int i = 0; i < n; i++)
    cin >> A[i] >> B[i] >> D[i];
  matrix_smew();
  return 0;
}
