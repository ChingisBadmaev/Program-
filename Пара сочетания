#include <iostream>
using namespace std;
int** C;
int* A;
int* B;
int* used;
int* M;
int n, m;
void uvel_cep(int v){
  if(used[v] == 1)
    return;
  int ma, k = 0;
  used[v] = 1;
  for(int i = 0; i < m; i++)
    if(C[v][i] == 1){
    	k++;
      uvel_cep(i);
      if(ma < M[i]+1)
        ma = M[i]+1;
    }
  if(k == 0)
    M[v] = 0;
  else M[v] = ma;
}
void matrix_smew(){
  C = new int*[m];
  for(int i = 0; i < m; i++)
    C[i] = new int[m];    
  for(int l = 0; l < m; l++)
    for(int i = 0; i < n; i++)
      if(A[i] == l)
        C[l][B[i]] = 1;
}
int main(){
  int ma, k;
  cin >> n >> m;//n-кол-во ребер, m-вершин
  A = new int[n];
  B = new int[n];
  M = new int[m];
  used = new int[m];
  for(int i = 0; i < n; i++)
    cin >> A[i] >> B[i];
  matrix_smew();
  for(int i = 0; i < m; i++)
    uvel_cep(i);
  for(int i = 0; i < m; i++)
    cout << M[i] << " ";
  cout << endl;
  /*for(int i = 0; i < m; i++)
    if(ma < M[i]){
      ma = M[i];
      k = i;
    }
  cout << M[k] << " ";
  M[k] = 0;
  ma = 0;
  for(int i = 0; i < m; i++){
    for(int l = 0; l < m; l++)
      if(ma < M[i] && C[k][l]){
        ma = M[l];
        k = l;
      }
    if(ma != 0){
      cout << M[k] << " ";
      M[k] = 0;
      ma = 0;
    }
  }*/
  return 0;
}
