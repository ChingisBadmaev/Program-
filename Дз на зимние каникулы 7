#include <iostream> 
#include <cmath> 
#include <algorithm>
using namespace std; 
void shortest_path(int** a, int n, int m){ 
  a[n][m] = min(a[n - 1][m], a[n][m - 1]) + 100;
} 
int main(){ 
 int N, M; 
 cin >> M >> N; 
 int** a = new int*[N]; 
 for (int i = 0; i < N; i++) 
  a[i] = new int[M]; 
 a[0][0] = 0; 
 for (int i = 1; i < N; i++) 
  a[i][0] = a[i - 1][0] + 100;
 for (int i = 1; i < M; i++) 
  a[0][i] = a[0][i - 1] + 100;
 for (int i = 1; i < N; i++)
  for (int l = 1; l < M; l++) 
   shortest_path(a, i, l);
 for (int i = 0; i < N; i++){
  for (int l = 0; l < M; l++) 
   cout << a[i][l] << " ";
  cout << endl;
 }
 return 0; 
}
