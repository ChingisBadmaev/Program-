#include <iostream>
using namespace std;
struct node{
  int data;
  int pri;
  node* next;
};
class queue{
 node* head;
  public:
   queue(int a, int i){
     head = new node;
     head -> data = a;
     head -> next = NULL;
     head -> pri = i;
   }
   void push(int a, int i){
     node* temp = head;
     node* temp1 = new node; 
     if (temp -> next == NULL){
      if (i < temp -> pri){
       temp1 -> next = NULL;
       temp -> next = temp1;
      }
      else temp1 -> next = temp;
      temp1 -> data = a;
      temp1 -> pri = i;
      head = temp1;
      return;
     }
     while (temp -> next -> pri > i && temp -> next != NULL)
      temp = temp -> next;
     temp1 -> next = temp -> next;
     temp1 -> data = a; 
     temp1 -> pri = i;
     temp -> next = temp1;
   }
   int pop(){
     node* temp = head;
     int a;
     if (temp -> next == NULL){
      a = temp -> data;
      delete temp;
      return a;
     }
     while(temp -> next -> next != NULL){
      temp = temp -> next;
     }
     a = temp -> next -> data;
     node* temp1 = temp -> next;
     temp -> next = NULL;
     delete temp1;
     return a;
   }
   ~queue(){
     while(head != NULL){
      node* temp = head;
      head = head -> next;
      delete temp;
     }
   }
};
int main(){
 int a, b, i1, i2;
 cin >> a >> b >> i1 >> i2;
 queue obj1(a, i1);
 obj1.push(b, i2);
 cout << obj1.pop();
return 0;
}
