#include <iostream>

using namespace std;

template<class T>
struct Node
{
	T d;
	Node<T> *n;
	Node(int dd, Node<T> *nn = nullptr) : d(dd), n(nn) {}
};

template<class T>
int length(Node<T> *f)
{
	Node *p = f;
	int i = 0;
	while (p != nullptr)
	{
		++i;
		p = p->n; 
	}
	return i;
}

