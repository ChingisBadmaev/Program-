#include <iostream>

using namespace std;

template<class T>
struct Node
{
	T d;
	Node<T> *n;
	Node(int dd = 0, Node<T> *nn = nullptr) : d(dd), n(nn) {}
};

template<class T>
Node<T> * cop(Node<T> *f, Node<T> *p = nullptr)
{
	if (f != nullptr) p->n = cop(f->n, p->n);
	p = new Node<T>(f->d);
	return p;
}


