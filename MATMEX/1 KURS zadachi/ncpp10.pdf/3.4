#include <iostream>

using namespace std;

template<class T>
struct Node
{
	T d;
	Node<T> *n;
	Node(int dd = 0, Node<T> *nn = nullptr) : d(dd), n(nn) {}
};

template<class T>
void del(Node<T> *&f)
{
	Node<T> *p;
	while (f != nullptr) 
	{
		p = f;
		f = f->n;
		delete p;
	}
}

