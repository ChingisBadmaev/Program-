#include <iostream>
using namespace std;

struct AA { 
	int x2, x3, x5;
	AA() { x2 = 0; x3 = 0; x5 = 0; }
	void rassmotr(int a) {
		for (int i = 0; i <= a; i++) {
			if (i % 2 == 0)
				x2++;
			if (i % 3 == 0)
				x3++;
			if (i % 5 == 0)
				x5++;
		}
	}
};

struct AB : AA{
	int x7;
	AB() { AA::AA(); x7 = 0; } 
	void rassmotr(int a) {
		AA::rassmotr(a);
		for (int i = 0; i < a; i++)
			if (i % 7 == 0)
				x7++;
	}
};

