#include <iostream>
#include <cmath>
using namespace std;

double &f(int n) {
	static double a = 2, x = 2;
	static int i = 1;
	x = pow(a, 1.0 / i);
	a = pow(x, n);
	switch (n) {
	case 1: return x;
	case 2: {
		i = n;
		return a;
	}
	case 3: {
		i = n;
		return a;
	}
	case 4: {
		i = n;
		return a;
	}
	}
}
int main() {
	int n;
	double m;
	for (int i = 0; i < 2; i++) {
		cin >> n >> m;
		f(n) = m;
		cout << f(1) << endl;
	}
	system("pause");
	return 0;
}
