      cout << 5 << " ";
      st1 << 5 << " ";
    }
    else{
    	cout << n << " ";
    	st1 << n << " ";
    }
  st.clear();
  st << st1.rdbuf();
  //cout << st.rdbuf();
	return 0;
}
