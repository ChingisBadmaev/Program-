#include <iostream>
using namespace std;

double f(double x);
void graph();

int main() {
	double x;
	cin >> x;
	cout << f(x) << endl;
	graph();
	system("pause");
	return 0;
}

f.cpp
#include <cmath>
double f(double x) {
	return 10*tan(x / 10);
}

graph.cpp
#include <iostream>
using namespace std;

double f(double x);

void graph() {
#include <iostream>
using namespace std;

double f(double x);

void graph() {
	for (double i = -100; i < 100; i += 0.5) {
		for (int l = 0; l < 40 + (int)(f(i) + 0.5); l++)
			cout << " ";
		if (abs((int)(f(i) + 0.5)) > 40)
			cout << " " << endl;
		else cout << "*" << endl;
	}
}
