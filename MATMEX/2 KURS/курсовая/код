#include <iostream>
#include <string>
#include <list>

using namespace std;

struct Object
{
	virtual ~Object() {}
};

struct vseml
{
	Object* obj;
};

struct Integer : Object
{
	int data;
	Integer(int a) :data(a) {}
};

struct Double : Object
{
	double data;
	Double(double a) :data(a) {}
};

struct String : Object
{
	string data;
	String(string a) :data(a) {}
};

struct List : Object
{
	list<vseml> data;
	List(list<vseml> a) { data = a; }
};

int main()
{

	system("pause");
	return 0;
}
