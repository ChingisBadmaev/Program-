#include <iostream>

using namespace std;

#define f1(x) ((x)*(x)) //функция x^2
#define f2(x, o) ((x)o(x)) //функция x действие x
#define f3(x) cout << #x << endl //вывод "x"
#define f4(x, y) cout << x##y << endl //вывод значения "x""y" без пробела

int main() {
	cout << f1(2) << endl;
	cout << f2(5, *) << endl;
	f3("+++");
	f4("one", "two");

	system("pause");
	return 0;
}

#undef f1 //удаление макроопределения
