#include <iostream>
using namespace std;

int *f() {} //возвращение из функции по значению
int *&f1() {} //возвращение из функции по ссылке

int main() {
	int *t, q = 11; //объявление *t
	t = &q; //прикрепление к t элемента q
	cout << *t << endl; //использование *t вместо q
	int r = *t; //использование t, вместо q
	cout << &q << " " << t << endl; //адрес ячейки памяти под q
	void *t1 = t; //неопределенный тип указателя(обязательно присвоить какой нибудь другой указатель)
	r += 16;
	*(int *)t1 = r; //применение неопределенного указателя
	system("pause");
	return 0;
}
