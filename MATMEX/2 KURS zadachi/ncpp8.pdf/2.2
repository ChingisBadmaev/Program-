#include <iostream>
#include <set>

using namespace std;

set <int> A;
int col = 1;

struct Foo
{
	int n;
	Foo() { n = col; A.insert(col); col++; cout << "create " << n << endl;}
	~Foo() { A.erase(n); cout << "del " << n << endl; }
};

int main()
{
	try { Foo A[5], *B = new Foo[5]; throw "qwerty"; }
	catch (const char *) {}
	system("pause");
	return 0;
}
