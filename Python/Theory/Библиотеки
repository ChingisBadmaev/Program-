# вместо #include
import random
for i in range(5):
   value = random.randint(1, 6)
   print(value) #от 1 до 6 включительно

#импорт нескольких функций
from math import cos, pi
print(pi)
print(cos(pi))

#импорт с изменением имени
from math import sqrt as square_root
print(square_root(100))

