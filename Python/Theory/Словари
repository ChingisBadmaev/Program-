#словари
ages = {"Dave": 24, "Mary": 42, "John": 58}
print(ages["Dave"])
ages["Dave"] = 13
print(ages["Dave"])
print(13 in ages)
#метод get
print(ages.get("John"))
print(ages.get("Vlad", "nety v ages"))

#кортежи
words = ("spam", "eggs", "sausages")
print(words[0])
words[1] = "cheese" #TypeError
#можно без скобок
my_tuple = "one", "two", "three"
print(my_tuple[0])
