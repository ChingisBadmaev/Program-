def comp_num_rectangle(n, m):
    ans = 0
    for i in range(n + 1):
        for j in range(m + 1):
            ans += i * j
    return ans

def main():
    print('Введите длину стороны квадрата:')
    n = int(input())
    print('Количество прямоугольников:')
    print(comp_num_rectangle(n, n))

if __name__ == '__main__':
    main()
