#include <iostream>
#include <vector>
using namespace std;
void outer_points(vector <int> &A, vector <int> &B, vector <int> &C, int n, int m){
	vector <int> D(m, n);
 for (int j = 0; j < n; j++) 
  for (int i = 0; i < m; i++)
   if (C[i]>=A[j] && C[i]<=B[j])
    D[i]--;
 for (int i = 0; i < m; i++)
  cout << D[i] << " ";
}
int main(){
	int n, m;
	cin >> n >> m;
	vector <int> A(n);
 vector <int> B(n);
 vector <int> C(m);
	for (int i = 0; i < n; i++)
	  cin >> A[i] >> B[i];
 for (int i = 0; i < m; i++)
   cin >> C[i];
 outer_points(A, B, C, n, m);
	return 0;
}
